{
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
			"src": "package.json",
			"use": "now-sapper"
		}
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
    ]
}